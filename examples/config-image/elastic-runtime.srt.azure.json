[
  {
    "version_ranges": {
      "pcf_version": ">=1.12.0 <2.0.0",
      "tile_version": ">=1.12.0 <2.0.0"
    },
    "config": {
      "properties": {
        ".cloud_controller.system_domain": {
          "value": "$SYSTEM_DOMAIN"
        },
        ".cloud_controller.apps_domain": {
          "value": "$APPS_DOMAIN"
        },
        ".cloud_controller.default_quota_memory_limit_mb": {
          "value": 102400
        },
        ".properties.networking_poe_ssl_cert": {
          "value": {
            "cert_pem": "$ERT_DOMAIN_CERT",
            "private_key_pem": "$ERT_DOMAIN_KEY"
          }
        },
        ".ha_proxy.skip_cert_verify": {
          "value": true
        },
        ".properties.haproxy_forward_tls": {
          "value": "disable"
        },
        ".router.request_timeout_in_seconds": {
          "value": 160
        },
        ".properties.security_acknowledgement": {
          "value": "X"
        },
        ".mysql_monitor.recipient_email": {
          "type": "email",
          "credential": false,
          "value": "pcf-iet@pivotal.io"
        },
        ".properties.container_networking": {
          "value": "enable"
        },
        ".uaa.service_provider_key_credentials": {
          "value": {
            "cert_pem": "$ERT_DOMAIN_CERT",
            "private_key_pem": "$ERT_DOMAIN_KEY"
          }
        },
        ".properties.syslog_host": {
          "value": "$JUMPBOX_PRIVATE_IP"
        },
        ".properties.syslog_port": {
          "value": 514
        },
        ".properties.syslog_protocol": {
          "value": "udp"
        },
        ".properties.syslog_tls": {
          "value": "disabled"
        },
        ".properties.cf_dial_timeout_in_seconds": {
          "value": 600
        },
        ".properties.gorouter_ssl_ciphers": {
          "value": "ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES256-GCM-SHA384"
        },
        ".properties.haproxy_ssl_ciphers": {
          "value": "DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES256-GCM-SHA384"
        }
      },
      "resource_config": {
        "router": {
          "instance_type": {
            "id": "automatic"
          },
          "instances": 2,
          "elb_names": [
            "$WEB_LB"
          ]
        },
        "compute": {
          "instances": 2
        }
      },
      "network": {
        "singleton_availability_zone": {
          "name": "$AZ_NAME"
        },
        "other_availability_zones": [
          {
            "name": "$AZ_NAME"
          }
        ],
        "network": {
          "name": "$ERT_NETWORK_NAME"
        }
      }
    }
  },
  {
    "version_ranges": {
      "pcf_version": ">=2.0.0",
      "tile_version": ">=2.0.0"
    },
    "config": {
      "properties": {
        ".cloud_controller.system_domain": {
          "value": "$SYSTEM_DOMAIN"
        },
        ".cloud_controller.apps_domain": {
          "value": "$APPS_DOMAIN"
        },
        ".cloud_controller.default_quota_memory_limit_mb": {
          "value": 102400
        },
        ".properties.networking_poe_ssl_certs": {
          "value": [
            {
              "name": "certificate",
              "certificate": {
                "cert_pem": "$ERT_DOMAIN_CERT",
                "private_key_pem": "$ERT_DOMAIN_KEY"
              }
            }
          ]
        },
        ".ha_proxy.skip_cert_verify": {
          "value": true
        },
        ".properties.haproxy_forward_tls": {
          "value": "disable"
        },
        ".router.request_timeout_in_seconds": {
          "value": 160
        },
        ".properties.security_acknowledgement": {
          "value": "X"
        },
        ".mysql_monitor.recipient_email": {
          "type": "email",
          "credential": false,
          "value": "pcf-iet@pivotal.io"
        },
        ".properties.container_networking": {
          "value": "enable"
        },
        ".properties.container_networking_interface_plugin.silk.network_mtu": {
          "value": 1438
        },
        ".properties.stack_migration_acknowledgement": {
          "value": "x"
        },
        ".uaa.service_provider_key_credentials": {
          "value": {
            "cert_pem": "$ERT_DOMAIN_CERT",
            "private_key_pem": "$ERT_DOMAIN_KEY"
          }
        },
        ".properties.syslog_host": {
          "value": "$JUMPBOX_PRIVATE_IP"
        },
        ".properties.syslog_port": {
          "value": 514
        },
        ".properties.syslog_protocol": {
          "value": "udp"
        },
        ".properties.syslog_tls": {
          "value": "disabled"
        },
        ".properties.cf_dial_timeout_in_seconds": {
          "value": 600
        },
        ".properties.gorouter_ssl_ciphers": {
          "value": "ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES256-GCM-SHA384"
        },
        ".properties.haproxy_ssl_ciphers": {
          "value": "DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES256-GCM-SHA384"
        },
        ".properties.credhub_key_encryption_passwords": {
          "value": [
            {
              "name": "credhub",
              "key": {
                "secret": "$CREDHUB_ENCRYPTION_PASSWORD"
              },
              "primary": 1
            }
          ]
        }
      },
      "resource_config": {
        "router": {
          "instance_type": {
            "id": "automatic"
          },
          "instances": 2,
          "elb_names": [
            "$WEB_LB"
          ]
        },
        "compute": {
          "instances": 2
        }
      },
      "network": {
        "singleton_availability_zone": {
          "name": "$AZ_NAME"
        },
        "other_availability_zones": [
          {
            "name": "$AZ_NAME"
          }
        ],
        "network": {
          "name": "$ERT_NETWORK_NAME"
        }
      }
    }
  }
]
